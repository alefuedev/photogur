<h2>Log in.</h2>
<%= form_tag sessions_path do %>
  <div class="field">
	  <%= label_tag :nickname %><br/>
	  <%= text_field_tag :nickname%><br/>
  </div>
  <div class="field">
		<%= label_tag :password %><br/>
		<%= password_field_tag :password%><br/>
</div>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>

